{
  "private": true,
  "name": "app",
  "version": "0.1.0",
  "scripts": {
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "eslint": "eslint ./app/javascript",
    "tslint": "tslint -p tsconfig.json -c tslint.json './app/javascript/**/*.{ts,tsx}' -t stylish",
    "lint": "yarn eslint && yarn tslint",
    "ts": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register -P tsconfig.json -O '{\"module\": \"commonjs\"}'",
    "test": "yarn ts ./node_modules/.bin/tape 'test/javascript/**/*.test.ts' | tap-spec",
    "verify-js": "yarn typecheck && yarn lint && yarn test",
    "verify-rb": "rubocop && rails test",
    "verify": "yarn verify-rb && yarn verify-js"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@rails/actioncable": "^6.0.0-alpha",
    "@rails/activestorage": "^6.0.0-alpha",
    "@rails/ujs": "^6.0.0-alpha",
    "@rails/webpacker": "^4.0.7",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "dotenv": "^8.1.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react_ujs": "^2.6.0",
    "slug": "^1.1.0",
    "ts-loader": "^6.1.2",
    "turbolinks": "^5.2.0",
    "typescript": "^3.6.3",
    "underscore.string": "^3.3.5",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.8",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "webpack-dev-server": "^3.8.1"
  }
}
